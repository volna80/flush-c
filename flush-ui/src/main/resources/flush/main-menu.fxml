<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="500.0" prefWidth="225.0"
            xmlns="http://javafx.com/javafx/8.0.66" fx:controller="com.volna80.flush.ui.controllers.MainMenuController">
    <children>
        <VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" prefHeight="500.0" prefWidth="225.0" spacing="10.0"
              xmlns="http://javafx.com/javafx/8">
            <children>
                <Button fx:id="instruments" mnemonicParsing="false" onAction="#onInstruments" prefWidth="100.0"
                        styleClass="button-standard" text="%main-menu.button.instruments"/>
                <Button fx:id="blotter" mnemonicParsing="false" onAction="#onBlotter" prefWidth="100.0"
                        styleClass="button-standard" text="%main-menu.button.blotter"/>
                <Button fx:id="preferences" mnemonicParsing="false" onAction="#onPreferences" prefWidth="100.0"
                        styleClass="button-standard" text="%main-menu.button.preferences"/>
                <Button fx:id="marketoverview" mnemonicParsing="false" onAction="#onMarketOverview" prefWidth="100.0"
                        styleClass="button-standard" text="%main-menu.button.market-overview"/>
            </children>
        </VBox>
    </children>
</AnchorPane>

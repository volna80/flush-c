<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0"
            prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.102-ea"
            fx:controller="com.volna80.flush.ui.controllers.MarketOverviewController">
    <top>
        <HBox alignment="CENTER_LEFT" BorderPane.alignment="CENTER">
            <children>
                <Label text="%market-overview.label.search">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </Label>
                <TextField fx:id="search" maxWidth="1.7976931348623157E308" prefHeight="26.0" prefWidth="547.0">
                    <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                    </HBox.margin>
                </TextField>
            </children>
        </HBox>
    </top>
    <center>
        <TableView xmlns:fx="http://javafx.com/fxml" fx:id="table" prefHeight="569.0"
                   prefWidth="1280.0"/>
    </center>
</BorderPane>
